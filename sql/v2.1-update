CREATE TABLE `cannal_inscricoes`.`alunos_operadoras_meta` (`aop_id` INT NOT NULL AUTO_INCREMENT , `aop_aluno` INT NOT NULL , `aop_operadora` VARCHAR(32) NOT NULL , `aop_key` VARCHAR(32) NOT NULL , `aop_value` TEXT NOT NULL, `aop_environment` VARCHAR(32) NOT NULL , PRIMARY KEY (`aop_id`)) ENGINE = InnoDB CHARSET=utf8mb4 COLLATE utf8mb4_unicode_ci;
ALTER TABLE `alunos_operadoras_meta` ADD CONSTRAINT `aop_operadora` FOREIGN KEY (`aop_operadora`) REFERENCES `operadoras`(`opr_nome`) ON DELETE CASCADE ON UPDATE CASCADE; ALTER TABLE `alunos_operadoras_meta` ADD CONSTRAINT `op_aluno` FOREIGN KEY (`aop_aluno`) REFERENCES `alunos`(`alu_id`) ON DELETE CASCADE ON UPDATE CASCADE;
ALTER TABLE `cannal_inscricoes`.`alunos_operadoras_meta` ADD UNIQUE `aop_alu_opr_key_env` (`aop_aluno`, `aop_operadora`, `aop_key`, `aop_environment`) USING BTREE;
